    "    with open(classes) as class_object:\n",
    "        for line in class_object:\n",
    "            vals = line.replace('\\n', '').split(' ')\n",
    "            key = vals[0]\n",
    "            vals.remove(key)\n",
    "            val = ' '.join(vals)\n",
    "            class_dic[key] = (val)\n",
    "            \n",
    "    with open(image_class_labels) as image_class_object:\n",
